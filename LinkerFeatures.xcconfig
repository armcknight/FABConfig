// Explicitly link Foundation to allow CLANG_MODULES_AUTOLINK to be set to NO.
OTHER_LDFLAGS[sdk=iphone*] = $(inherited) -framework Foundation -framework UIKit
OTHER_LDFLAGS[sdk=macosx*] = $(inherited) -framework Foundation
OTHER_LDFLAGS[sdk=watch*] = $(inherited) -framework Foundation -framework WatchKit
OTHER_LDFLAGS[sdk=appletv*] = $(inherited) -framework Foundation -framework UIKit 
